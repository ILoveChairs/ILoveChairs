{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "firestore.rules",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [ {
        "source": "**/*.@(woff2)",
        "headers": [ {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
        }, {
            "key": "Cache-Control",
            "value": "max-age=31536000"
        } ]
    }, {
        "source": "**/*.@(jpg|png|webp|gif|svg)",
        "headers": [ {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
        }, {
            "key": "Cache-Control",
            "value": "max-age=31536000"
        } ]
    }, {
        "source": "**/*.@(css|js)",
        "headers": [ {
            "key": "Cache-Control",
            "value": "max-age=86400, must-revalidate"
        } ]
    }, {
        "source": "404.html",
        "headers": [ {
            "key": "Cache-Control",
            "value": "max-age=300"
        } ]
    }, {
        "source": "**",
        "headers": [ {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; img-src *;"
        } ]
    }, {
        "source": "/",
        "headers": [ {
            "key": "Link",
            "value": "</scripts/index.js>;rel=preload;as=script;nopush,</styles/index.css>;rel=preload;as=style;nopush,</styles/atomic-font.css>;rel=preload;as=style;nopush"
        } ]
    } ]
  }
}